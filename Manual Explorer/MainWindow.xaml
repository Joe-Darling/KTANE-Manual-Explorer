            <Label Content="D Batteries:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,146,0,0"/>
            <Label Content="Battery Holders:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,177,0,0"/>
