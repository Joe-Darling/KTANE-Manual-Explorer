<Window x:Class="Manual_Explorer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Manual_Explorer"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1024">
    <Grid Background="#FF979797">
        <ComboBox x:Name="User_Query" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top" Width="195" Height="67" IsEditable="True" KeyDown="UpdateQuery" IsSynchronizedWithCurrentItem="False" IsTextSearchEnabled="False" SelectionChanged="OnSelected"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="10,152,0,0" VerticalAlignment="Top" Click="InitializeActiveModules"/>
        <Image x:Name="Page_1" HorizontalAlignment="Left" Height="550" Margin="280,44,0,0" VerticalAlignment="Top" Width="336"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="88,152,0,0" VerticalAlignment="Top" Click="LoadRandomFile"/>
        <Image x:Name="Page_2" HorizontalAlignment="Left" Height="550" Margin="638,44,0,0" VerticalAlignment="Top" Width="336"/>

    </Grid>
</Window>
