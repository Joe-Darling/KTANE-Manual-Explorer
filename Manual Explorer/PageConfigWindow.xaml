<Window x:Class="Manual_Explorer.PageConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Manual_Explorer"
        mc:Ignorable="d"
        Title="PageConfigWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="Read_In_Modules" HorizontalAlignment="Left" Margin="81,191,0,0" VerticalAlignment="Top" Width="195" Height="26" IsEditable="True" KeyDown="UpdateQuery" IsSynchronizedWithCurrentItem="False" IsTextSearchEnabled="False" KeyUp="BackspaceUpdate" SelectionChanged="SetDownloadedComboText"/>
        <ComboBox x:Name="Downloaded_Modules" HorizontalAlignment="Left" Margin="485,191,0,0" VerticalAlignment="Top" Width="195" Height="26" IsEditable="True" KeyDown="UpdateQuery" IsSynchronizedWithCurrentItem="False" IsTextSearchEnabled="False" SelectionChanged="OnSelected" KeyUp="BackspaceUpdate"/>
    </Grid>
</Window>
